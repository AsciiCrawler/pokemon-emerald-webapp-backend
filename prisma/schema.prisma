generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model pokemon {
  uuid            String @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  identifier      String @db.String(100)
  image           String @db.String(100)
  gif             String @db.String(100)
  height          Int
  weight          Int
  base_experience Int
  id              Int    @unique
}
